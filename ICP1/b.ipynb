   "source": [
    "a=int(input(\"input the first number:\"))\n",
    "m=input(\"choice +-*/\")\n",
    "b=int(input(\"input the second number:\"))\n",
    "if m=='+':\n",
    "    print(a+b)\n",
    "elif m=='-':\n",
    "    print(a-b)\n",
    "elif m=='*':\n",
    "    print(a*b)\n",
    "elif m=='/':\n",
    "    if b==0:\n",
    "        print('error')\n",
    "    else:\n",
    "        print(a/b)\n",
    "else:print('error')\n"
